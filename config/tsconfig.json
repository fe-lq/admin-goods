{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES6", "DOM"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": false,
    "skipDefaultLibCheck": false,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": "./"
  },
  "include": ["./"],
  "exclude": ["node_modules"]
}
